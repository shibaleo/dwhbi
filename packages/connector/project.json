{
  "name": "connector",
  "projectType": "library",
  "sourceRoot": "packages/connector/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/connector",
        "command": "tsc"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/connector",
        "command": "tsc --noEmit"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/connector",
        "command": "vitest run"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/connector",
        "command": "npx tsc --noEmit"
      }
    },
    "sync:toggl": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/connector",
        "command": "npx tsx src/services/toggl-track/cli.ts"
      }
    },
    "sync:gcal": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/connector",
        "command": "npx tsx src/services/google-calendar/cli.ts"
      }
    }
  },
  "tags": ["scope:connector", "type:lib", "lang:typescript"]
}
